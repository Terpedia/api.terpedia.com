{
  "version": 2,
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.2.7"
    }
  },
  "rewrites": [
    {
      "source": "/v1/models",
      "destination": "/api/v1/models"
    },
    {
      "source": "/v1/chat/completions",
      "destination": "/api/v1/chat/completions"
    },
    {
      "source": "/v1/:path*",
      "destination": "/api/v1/:path*"
    }
  ]
}
